{
  "name": "mood-city-guide-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3007",
    "dev:turbo": "next dev --turbopack -p 3007",
    "build": "prisma generate && SKIP_ENV_VALIDATION=true next build",
    "build:production": "NODE_ENV=production next build -c next.config.production.js",
    "start": "next start -p 3007",
    "start:production": "NODE_ENV=production next start -p 3007",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate:production": "npx prisma migrate deploy --schema=prisma/schema.production.prisma",
    "db:generate:production": "npx prisma generate --schema=prisma/schema.production.prisma",
    "db:migrate:supabase": "npx prisma migrate deploy --schema=prisma/schema.supabase.prisma",
    "db:generate:supabase": "npx prisma generate --schema=prisma/schema.supabase.prisma",
    "db:push:supabase": "npx prisma db push --schema=prisma/schema.supabase.prisma",
    "migrate:to-supabase": "node scripts/migrate-to-supabase.js",
    "setup:production": "./scripts/setup-production.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:production": "./scripts/deploy.sh --production",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.540.0",
    "multer": "^2.0.2",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
